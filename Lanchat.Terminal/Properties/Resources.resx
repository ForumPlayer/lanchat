    </data>

    <!-- Help / Commands summary -->
    <data name="Summary_connect" xml:space="preserve">
        <value>
    Connect with other users.
        </value>
    </data>
    <data name="Summary_nick" xml:space="preserve">
        <value>
    Changes user nickname.
    Nickname have to be between 1-20 characters.
    Nickname cannot contains spaces.
        </value>
    </data>
    <data name="Summary_block" xml:space="preserve">
        <value>
    Prevent from connecting to specified user.
    If you are not currently connected you can block by IP address.
        </value>
    </data>
    <data name="Summary_unblock" xml:space="preserve">
        <value>
    Unblock user. You have to user IP address in argument.
    Check /blocked command for blocked addresses list.
        </value>
    </data>
    <data name="Summary_blocked" xml:space="preserve">
        <value>
    Show list of blocked IP addresses.
        </value>
    </data>
    <data name="Summary_send" xml:space="preserve">
        <value>
    Send file. Make sure path in argument doesn't have quotation marks.
    If the target user accepts the request, the transfer will begin.
        </value>
    </data>
    <data name="Summary_accept" xml:space="preserve">
        <value>
    Accept incoming file transfer request.
        </value>
    </data>
    <data name="Summary_reject" xml:space="preserve">
        <value>
    Reject incoming file transfer request.
        </value>
    </data>
    <data name="Summary_cancel" xml:space="preserve">
        <value>
    Cancel file transfer.
        </value>
    </data>
    <data name="Summary_afk" xml:space="preserve">
        <value>
    Change status to "Away From Keyboard"
        </value>
    </data>
    <data name="Summary_dnd" xml:space="preserve">
        <value>
    Change status to "Do Not Disturb"
        </value>
    </data>
    <data name="Summary_online" xml:space="preserve">
        <value>
    Change status to "Online"
        </value>
    </data>
    <data name="Summary_whois" xml:space="preserve">
        <value>
    Show information about user
        </value>
    </data>
    <data name="Summary_trust" xml:space="preserve">
        <value>
    Mark user as trusted.
    If user is trusted Lanchat will connect with addresses from list received from this user.
    List of trusted users persists in "nodes.json" file.
        </value>
    </data>

    <!-- Help / Commands syntax -->
    <data name="Syntax_connect" xml:space="preserve">
        <value>
    /connect [ip]
    /connect [ip] [port]
    /connect [domain]
    /connect [domain] [port]
        </value>
    </data>
    <data name="Syntax_disconnect" xml:space="preserve">
        <value>
    /disconnect [ID]
        </value>
    </data>
    <data name="Syntax_nick" xml:space="preserve">
        <value>
    /nick [new nick]
        </value>
    </data>
    <data name="Syntax_block" xml:space="preserve">
        <value>
    /block [ID]
    /block [IP address]
        </value>
    </data>
    <data name="Syntax_unblock" xml:space="preserve">
        <value>
    /unblock [IP address]
        </value>
    </data>
    <data name="Syntax_send" xml:space="preserve">
        <value>
    /send [ID] [file path]
        </value>
    </data>
    <data name="Syntax_accept" xml:space="preserve">
        <value> 
    /accept [ID]
        </value>
    </data>
    <data name="Syntax_reject" xml:space="preserve">
        <value>
    /reject [ID]
        </value>
    </data>
    <data name="Syntax_cancel" xml:space="preserve">
        <value>
    /cancel [ID]
        </value>
    </data>
    <data name="Syntax_help" xml:space="preserve">
        <value>
